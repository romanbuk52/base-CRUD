//  edit user
curl --header "Content-Type: application/json"   --request PUT   --data '{"firstName":"xyz","lastName":"xyz","height":123,"weight":321}' -o - http://localhost:8080/man/ea323b32-4600-438f-94f2-dbba2d38af7c

// get all
curl --header "Content-Type: application/json"   --request POST  http://localhost:8080/man/all

//  add users
curl --header "Content-Type: application/json"   --request POST   --data '{"firstName":"koli","lastName":"marlo","height":123,"weight":321}' -o - http://localhost:8080/man
curl --header "Content-Type: application/json"   --request POST   --data '{"firstName":"Andriy","lastName":"xyz","height":900,"weight":900}' http://localhost:8080/man
curl --header "Content-Type: application/json"   --request POST   --data '{"firstName":"Bogdan","lastName":"xyz","height":1000,"weight":1000}' http://localhost:8080/man


curl --request DELETE -o - http://localhost:8080/man/577
